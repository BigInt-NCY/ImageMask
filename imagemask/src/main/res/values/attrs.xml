<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="MaskMapImageView">
		<attr name="masks" format="reference"/>
		<attr name="mask_type" format="enum">
			<!-- -->
			<enum name="opacity" value="1"/>
			<enum name="transparency" value="2"/>
			<enum name="includeColor" value="3"/>
			<enum name="excludeColor" value="4"/>
		</attr>
		<attr name="mask_color" format="color"/>
		<attr name="mask_weight" format="float"/>
		<attr name="mask_showDebugOverlay" format="boolean"/>
	</declare-styleable>
</resources>